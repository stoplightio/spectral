{
  "rules": {
    "PascalCase": {
      "severity": "warn",
      "given": "$",
      "message": "bar",
      "then": {
        "function": "truthy"
      }
    },
    "camelCase": {
      "severity": "warn",
      "given": "$",
      "recommended": true,
      "message": "bar",
      "then": {
        "function": "truthy"
      }
    }
  }
}
