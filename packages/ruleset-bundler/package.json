    "@stoplight/spectral-ref-resolver": ">=1",
    "@stoplight/spectral-rulesets": ">=1",
    "@stoplight/spectral-runtime": "^1.1.0",
    "@stoplight/types": "12.3.0",
    "@types/node": "*",
    "rollup": "~2.56.3",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@types/validate-npm-package-name": "^3.0.3",
    "fetch-mock": "^9.11.0",
