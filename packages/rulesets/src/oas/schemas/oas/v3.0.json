        },
        "description": {
          "type": "string"
        }
      },
      "patternProperties": {
        "^x-": {}
      },
      "additionalProperties": false
    },
    "Components": {
      "type": "object",
      "properties": {
        "schemas": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Schema"
              }
            }
          }
        },
        "responses": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Response"
              }
            }
          }
        },
        "parameters": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Parameter"
              }
            }
          }
        },
        "examples": {
          "type": "object",
