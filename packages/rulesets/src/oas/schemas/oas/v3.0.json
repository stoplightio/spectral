            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Example"
              }
            }
          }
        },
        "requestBodies": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/RequestBody"
              }
            }
          }
        },
        "headers": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Header"
              }
            }
          }
        },
        "securitySchemes": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/SecurityScheme"
              }
            }
          }
        },
        "links": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Link"
              }
            }
          }
        },
        "callbacks": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9\\.\\-_]+$": {
              "if": {
                "type": "object",
                "required": ["$ref"]
              },
              "then": {
                "$ref": "#/definitions/Reference"
              },
              "else": {
                "$ref": "#/definitions/Callback"
              }
            }
          }
        }
      },
      "patternProperties": {
        "^x-": {}
      },
      "additionalProperties": false
